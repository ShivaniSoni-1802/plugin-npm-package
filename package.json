{
	"name": "@graphql-codegen/typescript",
	"version": "2.4.1",
	"description": "Plugin for code Graphql Code Generator, to translate gql enums into smart enums",
	"author": "WEB DEV",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/ShivaniSoni-1802/plugin-npm-package.git"
	},
	"license": "MIT",
	"scripts": {
		"build": "tsc --project .",
		"deploy:tsn": "ts-node ./src/index.ts",
		"tsc": "tsc --build",
		"lint": "eslint **/*.ts",
		"test": "jest --no-watchman --config ../../../../jest.config.js"
	},
	"dependencies": {
		"@graphql-codegen/plugin-helpers": "^2.3.0",
		"@graphql-codegen/schema-ast": "^2.4.0",
		"@graphql-codegen/testing": "^1.17.7",
		"@graphql-codegen/visitor-plugin-common": "2.5.1",
		"@types/jest": "^27.0.3",
		"auto-bind": "^5.0.1",
		"cypress": "^9.1.0",
		"graphql": "^16.0.1",
		"tslib": "~2.3.0",
		"typescript": "^4.5.2"
	},
	"peerDependencies": {
		"graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"require": "./dist/*.js",
			"import": "./dist/*.mjs"
		}
	},
	"typings": "dist/index.d.ts",
	"typescript": {
		"definition": "dist/index.d.ts"
	},
	"buildOptions": {
		"input": "./src/index.ts"
	},
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	}
}
